lidateType from \"filepond-plugin-file-validate-type\";\nconst FilePond: any = vueFilePond(\n  FilePondPluginFileValidateType,\n  FilePondPluginImagePreview\n);\nimport { mapActions } from \"vuex\";\nexport default defineComponent({\n  data() {\n    return {\n      coursePreview: [] as any,\n    };\n  },\n  components: {\n    FilePond,\n  },\n  methods: {\n    ...mapActions({\n      uplodaeCourse: \"course/uplodaeCourse\",\n    }),\n    handleFilePondInit(event: any) {\n      this.coursePreview = event.target.files[0];\n    },\n    UploadeVideo() {\n      let formData = new FormData();\n      formData.append(\"contentMetadata\", \"2\");\n      formData.append(\"file\", this.coursePreview);\n      this.uplodaeCourse(formData);\n \n    },\n  },\n});\n</script>\n<style lang=\"scss\">\n.filepond--wrapper {\n  a.filepond--credits {\n    display: none;\n  }\n}\n</style>\n"]}]}