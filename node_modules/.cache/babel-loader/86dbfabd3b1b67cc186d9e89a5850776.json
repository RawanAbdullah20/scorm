components","FirstStep","SecondStep","ThirdStep","Wizard","WizardTab","methods","validateStep","ref","onStepValidated","validated","model","wizardComplete","Swal","title","text","type","confirmButtonClass","buttonsStyling"],"sourceRoot":"src/components/Dashboard/Views/Forms","sources":["Wizard.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"row d-flex justify-content-center\">\n      <div class=\"col-md-10 mr-auto ml-auto\">\n          <wizard @complete=\"wizardComplete\">\n            <template slot=\"header\">\n              <h3 class=\"card-title\">Build your profile</h3>\n              <h3 class=\"description\">This information will let us know more about you.</h3>\n            </template>\n\n            <wizard-tab :before-change=\"() => validateStep('step1')\">\n              <template slot=\"label\">\n                <i class=\"nc-icon nc-single-02\"></i>\n                About\n              </template>\n              <first-step ref=\"step1\" @on-validated=\"onStepValidated\"></first-step>\n            </wizard-tab>\n\n            <wizard-tab :before-change=\"() => validateStep('step2')\">\n              <template slot=\"label\" >\n                <i class=\"nc-icon nc-touch-id\"></i>\n                Account\n              </template>\n              <second-step ref=\"step2\" @on-validated=\"onStepValidated\"></second-step>\n            </wizard-tab>\n\n            <wizard-tab :before-change=\"() => validateStep('step3')\">\n              <template slot=\"label\">\n                <i class=\"nc-icon nc-pin-3\"></i>\n                Address\n              </template>\n              <third-step ref=\"step3\"></third-step>\n            </wizard-tab>\n          </wizard>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\n  import FirstStep from './Wizard/FirstStep.vue'\n  import SecondStep from './Wizard/SecondStep.vue'\n  import ThirdStep from './Wizard/ThirdStep.vue'\n  import Swal from 'sweetalert2'\n  import {Wizard, WizardTab} from 'src/components/UIComponents'\n\n  export default {\n    data() {\n      return {\n        wizardModel: {}\n      }\n    },\n    components: {\n      FirstStep,\n      SecondStep,\n      ThirdStep,\n      Wizard,\n      WizardTab\n    },\n    methods: {\n      validateStep(ref) {\n        return this.$refs[ref].validate()\n      },\n      onStepValidated(validated, model) {\n        this.wizardModel = {...this.wizardModel, ...model}\n      },\n      wizardComplete() {\n        Swal.fire({\n          title: \"Good job!\",\n          text: \"You clicked the finish button!\",\n          type: \"success\",\n          confirmButtonClass: \"btn btn-success\",\n          buttonsStyling: false\n        });\n      }\n    }\n  }\n</script>\n"]}]}