  };\n    }\n  },\n  mounted() {\n    this.addPlugin(pluginChartText);\n    this.chartData = this.assignChartData({});\n    this.options = this.assignChartOptions(defaultOptions);\n    this.options.elements.center.text = this.data ? `${this.data[0]}%` : null;\n    this.renderChart(this.chartData, this.options);\n  }\n}\n"],"mappings":";AAAA,SAASA,GAAT,QAAoB,aAApB;AACA,OAAOC,eAAP,MAA4B,6BAA5B;AAEA,IAAMC,cAAc,GAAG;EACrBC,QAAQ,EAAE;IACRC,MAAM,EAAE;MACNC,IAAI,EAAE,KADA;MAENC,KAAK,EAAE,SAFD;MAGNC,SAAS,EAAE,OAHL;MAINC,WAAW,EAAE;IAJP;EADA,CADW;EASrBC,gBAAgB,EAAE,EATG;EAUrBC,MAAM,EAAE;IAENC,OAAO,EAAE;EAFH,CAVa;EAerBC,QAAQ,EAAE;IACRC,OAAO,EAAE;EADD,CAfW;EAmBrBC,MAAM,EAAE;IACNC,KAAK,EAAE,CAAC;MAENC,KAAK,EAAE;QACLL,OAAO,EAAE;MADJ,CAFD;MAKNM,SAAS,EAAE;QACTC,UAAU,EAAE,KADH;QAETC,aAAa,EAAE,aAFN;QAGTb,KAAK,EAAE;MAHE;IALL,CAAD,CADD;IAcNc,KAAK,EAAE,CAAC;MACNC,aAAa,EAAE,GADT;MAENJ,SAAS,EAAE;QACTC,UAAU,EAAE,KADH;QAETZ,KAAK,EAAE,uBAFE;QAGTa,aAAa,EAAE;MAHN,CAFL;MAONH,KAAK,EAAE;QACLL,OAAO,EAAE;MADJ;IAPD,CAAD;EAdD;AAnBa,CAAvB;AA+CA,eAAe;EACbW,IAAI,EAAE,gBADO;EAEbC,OAAO,EAAEvB,GAFI;EAGbwB,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAE,CAACC,MAAD,EAASC,KAAT,CADA;MAENC,WAAW,EAAE;IAFP,CADH;IAKLC,QAAQ,EAAE;MACRJ,IAAI,EAAE,CAACC,MAAD,EAASC,KAAT,CADE;MAERC,WAAW,EAAE;IAFL,CALL;IASLE,IAAI,EAAE;MACJL,IAAI,EAAE,CAACC,MAAD,EAASC,KAAT,CADF;MAEJC,WAAW,EAAE;IAFT,CATD;IAaLvB,KAAK,EAAE;MACLoB,IAAI,EAAEM,MADD;MAELH,WAAW,EAAE;IAFR,CAbF;IAiBLI,YAAY,EAAE;MACZP,IAAI,EAAEC,MADM;MAEZE,WAAW,EAAE;IAFD,CAjBT;IAqBLK,KAAK,EAAE;MACLR,IAAI,EAAEM,MADD;MAELH,WAAW,EAAE;IAFR;EArBF,CAHM;EA6BbM,OAAO,EAAE;IACPC,eADO,6BACW;MAChB,OAAO;QACLX,MAAM,EAAE,KAAKA,MAAL,IAAe,EADlB;QAELK,QAAQ,EAAE,KAAKA,QAAL,GAAgB,KAAKA,QAArB,GAAgC,CAAC;UACzCO,WAAW,EAAE,CAD4B;UAEzCC,gBAAgB,EAAE,CAFuB;UAGzCC,eAAe,EAAE,CAAC,KAAKjC,KAAL,IAAc,SAAf,EAA0B,KAAKkC,cAAL,IAAuB,SAAjD,CAHwB;UAIzCC,WAAW,EAAE,CAJ4B;UAKzCV,IAAI,EAAE,KAAKA,IAAL,IAAa;QALsB,CAAD;MAFrC,CAAP;IAUD,CAZM;IAaPW,kBAbO,8BAaYC,aAbZ,EAa2B;MAChC,IAAIV,YAAY,GAAG,KAAKA,YAAL,IAAqB,EAAxC;MACA,uCACKU,aADL,GAE